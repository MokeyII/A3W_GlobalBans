<Window x:Class="A3W_Bans.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:A3W_Bans"
        mc:Ignorable="d"
        Title="Login" Height="300" Width="300" Background="#FF7E7D7D">
    <Grid>
        <Button x:Name="btnSqlLogin" Content="Login" Margin="112,228,105,21" Click="btnSqlLogin_Click"/>
        <TextBox x:Name="txtSqlPassword" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="Password" VerticalAlignment="Top" Width="120" Margin="93,198,0,0"/>
        <TextBox x:Name="txtSqlUserName" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="Username" VerticalAlignment="Top" Width="120" Margin="93,161,0,0"/>
        <Label x:Name="lblUserName" Content="UserName:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="20,158,0,0"/>
        <Label x:Name="lblPassword" Content="Password:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="26,195,0,0" RenderTransformOrigin="0.515,0.628"/>

    </Grid>
</Window>
